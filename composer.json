{
  "name": "arraypress/wp-post-type-utils",
  "version": "1.0.0",
  "description": "A lean WordPress library for working with post types and post type operations",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-post-type-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\PostTypeUtils\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-post-type-utils/issues",
    "source": "https://github.com/arraypress/wp-post-type-utils"
  },
  "keywords": [
    "wordpress",
    "post-types",
    "wp",
    "arraypress",
    "post-type-object",
    "capabilities",
    "search",
    "options"
  ]
}